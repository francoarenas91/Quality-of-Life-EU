data <- read_csv("./Music.csv")
data %>% str()
data %>% summary()
data %>%
select(year, speech) %>%
mutate(year=as.factor(year))-> data
boxplot(speech ~ year,data)
print(paste0("the means of the groups are"))
tapply(data$speech, data$year, mean)
print(paste0("the means of the groups are"))
tapply(data$speech, data$year, sd)
aovOut <- aov(speech ~ year,data)
summary(aovOut)
TukeyHSD(aovOut)
# install.packages("BayesFactor")
